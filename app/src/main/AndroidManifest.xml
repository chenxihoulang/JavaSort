<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.chaihongwei.javasort"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".InsertSortActivity">
        </activity>
        <activity android:name=".ShellSortActivity">
        </activity>
        <activity android:name=".BubbleSortActivity">
        </activity>
        <activity android:name=".SelectSortActivity">
        </activity>
        <activity android:name=".QuickSortActivity">
        </activity>
        <activity android:name=".MergeSortActivity">
        </activity>
        <activity android:name=".BinaryInsertionSortActivity">
        </activity>
        <activity android:name=".HeapSortActivity">
        </activity>
        <activity android:name=".RadixSortActivity">
        </activity>
    </application>

</manifest>